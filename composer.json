{
    "name": "digit-soft/laravel-i18n",
    "description": "Laravel i18n component.",
    "keywords": ["laravel i18n db", "i18n", "translation", "db translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Digit",
            "email": "digit.vova@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "~5.6"
    },
    "autoload": {
        "psr-4": {
            "DigitSoft\\LaravelI18n\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "DigitSoft\\LaravelI18n\\TranslationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
